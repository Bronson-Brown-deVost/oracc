        <element name="db_source">
          <text/>
        </element>
        <element name="designation">
          <ref name="sortcode"/>
          <text/>
        </element>
        <element name="display_size">
          <text/>
        </element>
        <element name="electronic_publication">
          <text/>
        </element>
        <element name="elevation">
