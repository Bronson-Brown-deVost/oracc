<?xml version="1.0" encoding="UTF-8"?>
<grammar ns="http://oracc.org/ns/XMD/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <start>
    <element name="xmd">
